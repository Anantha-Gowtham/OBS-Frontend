# Frontend Environment Variables Template
# Copy this file to .env.development for development
# Copy this file to .env.production for production

# API Configuration
VITE_API_BASE_URL=http://localhost:8085/api
VITE_WEBSOCKET_URL=ws://localhost:8085/ws

# Application Configuration
VITE_APP_NAME=OBS Banking System
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_DEMO_MODE=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=false

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_THEME_SWITCHER=true

# Security Configuration (Development Only)
VITE_ENABLE_SECURITY_LOGS=false
VITE_SESSION_TIMEOUT=30